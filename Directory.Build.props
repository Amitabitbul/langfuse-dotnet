<Project>
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <Authors>Amit Abitbul</Authors>
    <Copyright>Copyright Â© 2025</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Amitabitbul/langfuse-dotnet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>langfuse;opentelemetry;otel;observability;semantic-kernel;genai;tracing;llm;prompts</PackageTags>
    <PackageProjectUrl>https://github.com/Amitabitbul/langfuse-dotnet</PackageProjectUrl>
  </PropertyGroup>

  <!-- Common package versions -->
  <PropertyGroup>
    <MicrosoftExtensionsVersion>8.0.0</MicrosoftExtensionsVersion>
    <OpenTelemetryVersion>1.9.0</OpenTelemetryVersion>
  </PropertyGroup>
</Project>
